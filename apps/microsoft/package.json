{
  "name": "elba-microsoft",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3010",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "sh ./scripts/run-test.sh",
    "test:watch": "sh ./scripts/run-test.sh -w",
    "test:database:up": "docker-compose -p integration-test --env-file=.env.test up -d",
    "test:database:down": "docker-compose -p integration-test down",
    "test:database:migrate": "node ./scripts/migrate.js .env.test",
    "database": "docker-compose up -d",
    "database:migrate": "node ./scripts/migrate.js .env",
    "generate:migration": "npx drizzle-kit generate:pg",
    "inngest:dev": "pnpm inngest-cli dev",
    "database:studio": "npx drizzle-kit studio"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.28.6",
    "@elba-security/sdk": "workspace:^",
    "inngest": "^3.6.1",
    "jose": "^5.1.0",
    "next": "14.0.1",
    "postgres": "^3.4.3",
    "react": "^18",
    "react-dom": "^18",
    "ui": "*",
    "vercel": "^32.5.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.4.19",
    "@types/microsoft-graph": "^2.38.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.16",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8",
    "@elba-security/eslint-config-custom": "workspace:*",
    "inngest-cli": "^0.21.1",
    "msw": "^2.0.7",
    "pg": "^8.11.3",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "@elba-security/tsconfig": "workspace:*",
    "typescript": "^5",
    "vitest": "^1.0.0-beta.5",
    "@elba-security/test-utils": "workspace:*"
  }
}
