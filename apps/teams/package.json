{
  "name": "teams",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4000",
    "dev:inngest": "inngest-cli dev -u http://localhost:4000/api/inngest",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "sh ./scripts/run-test.sh",
    "test:watch": "sh ./scripts/run-test.sh -w",
    "test:database:up": "docker-compose -p integration-test --env-file=.env.test up -d",
    "test:database:down": "docker-compose -p integration-test down",
    "test:database:migrate": "node ./scripts/migrate.js .env.test",
    "database:up": "docker-compose -p integration --env-file=.env.local up -d",
    "database:down": "docker-compose -p integration down",
    "database:migrate": "node ./scripts/migrate.js .env.local",
    "database:generate": "drizzle-kit generate:pg --schema=./src/database/schema.ts"
  },
  "dependencies": {
    "next": "14.0.4",
    "next-auth": "^4.24.5",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
